{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "astro-storefront",
  "compatibility_date": "2025-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "main": "dist/_worker.js",
  "assets": {
    "directory": "dist",
    "binding": "ASSETS"
  },
  "services": [
    {
      "binding": "PAYLOAD_CMS",
      "service": "astro-payload-ecom"
    }
  ]
}
